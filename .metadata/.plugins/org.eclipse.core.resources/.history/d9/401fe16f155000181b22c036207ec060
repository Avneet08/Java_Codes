import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collections;
import java.util.Scanner;

public class rw {

	public static void main(String args[]) {
		try {
			Scanner sc = new Scanner(System.in);
			int T = sc.nextInt();
			for (int j = 0; j < T; j++) {
				int limit = sc.nextInt();
				Long arr[] = new Long[limit];
				for (int i = 0; i < limit; i++) {
					arr[i] = sc.nextLong();
				}
				ArrayList<Long> C=generateseq(arr,arr.length);
				//System.out.println(Arrays.toString(C));
				ArrayList<Long> dup2=dup(C);
				long Xor=0;
				for(int i=0;i<dup2.size();i++) {
					Xor=xoring(Xor,dup2.get(i));
				}
				System.out.println(Xor);
			}
		} catch (Exception e) {
			return;
		}
}
	public static ArrayList<Long> dup(ArrayList<Long> arr) {
		Collections.sort(arr); 
		//Arrays.sort(arr);
		ArrayList<Long> arr2 = new ArrayList<Long>();
		arr2.add(arr.get(0));
		for (int i = 1; i < arr.size(); i++) {
			
			if (arr.get(i)== arr.get(i-1)) {
				// System.out.println("Duplicate: " + arr[i]);
				continue;
			} else {
				arr2.add(arr.get(i));
			}
		}
		
		return arr2;

	}

	public static long xoring(long xor,long c){
		return (xor^c);
		}
	public static ArrayList<Long> generateseq(Long[] arr,int n) {
		//long[] B=new long[n*n];
		ArrayList<Long> matrix=new ArrayList<Long>();
		for(int i=0;i<n;i++) {
			for(int j=0;j<n;j++) {
				matrix.add(arr[i]+arr[j]);
				//B[n*i+j]=arr[i]+arr[j];
				
			}
		}
		return matrix;
		
		
	}
}