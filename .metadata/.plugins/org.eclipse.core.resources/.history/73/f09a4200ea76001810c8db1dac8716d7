import java.util.Scanner;

public class J1_p1 {
	static int count( int S[], int m, int n )
	{
		if (n == 0)
			return 1;
		if (n < 0)
			return 0;
		if (m <=0 && n >= 1)
			return 0;
		return count( S, m - 1, n ) +
			count( S, m, n-S[m-1] );
	}
	
	
	public static void main(String[] args) {
	
		
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		for (int i = 0; i < n; i++) {
			int t = sc.nextInt();
			int arr[] = {7, 3};
			
			if( count(arr, 2, t)==0) {
				System.out.println("NO");
			}
			else {
				System.out.println("YES");
			}
			
			
	}
	}

}
