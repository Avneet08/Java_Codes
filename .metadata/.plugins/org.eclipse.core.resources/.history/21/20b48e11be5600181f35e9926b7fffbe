import java.math.BigInteger;
import java.util.*;
class Marker{
	int index_a;
	int index_b;
	public Marker(int index_a,int index_b) {
		this.index_a=index_a;
		this.index_b=index_b;
	}
	
}

public class FAKEBS {
	private static void check(int left_moves,int b_index ,int right_moves,int right) {
		if(left_moves>b_index || right_moves>right){
	    	System.out.println("-1");
	    }
	    else{
	    	System.out.println( left_moves + right_moves);
	    }
	}
private static int Binarysearch(BigInteger[] arr, BigInteger arr2) {
		
	    int low = 0;
	    int high = arr.length;
	    int mid =0;
	    while(low <= high){
	    	mid = (low + high) / 2;
			        if(arr[mid] .compareTo(arr2)==0){
			            break;
			        }
			        else if(arr[mid] .compareTo(arr2) ==1){
			        	high = mid-1;
			            
			        }
			        else{
			        	low = mid+1;
			        }
	    }
	    return mid;
}
		
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		int u=0;
		
			while(u<T) {
			int N = sc.nextInt();
			int Q = sc.nextInt();
			BigInteger arr[] = new BigInteger[N];
			
			HashMap<BigInteger,Marker> hashmap = new HashMap<BigInteger,Marker>();
			BigInteger arr2[] = new BigInteger[N];
			for(int i =0;i<N;i++){
				arr[i] = sc.nextBigInteger(); 
				arr2[i]=arr[i];
				
				
			}
			Arrays.sort(arr);
			
			for(int i =0;i<N;i++){
				int v=Binarysearch(arr,arr[i]);
				hashmap.put(arr2[i], new Marker(i, v));
			}
			int i=0;
			
			while(i<Q){
				BigInteger x = sc.nextBigInteger();
				//int marker=findx(arr2,N,x);
				Marker a=hashmap.get(x);
				System.out.println(a.index_a+" "+a.index_b);
				//makemoves(arr2,arr,x,a.index_a,a.);
				
				
			
			i++;	
			}
		u++;
		}
	}
	private static int findx(BigInteger arr2[],int N,Long x) {
		int marker= -1;
		for(int i =0;i<N;i++){
			if(arr2[i].compareTo(BigInteger.valueOf(x))==0){
				marker = i;
				break;
			}
			
		}
		return marker;
	}
	private static void makemoves(BigInteger arr2[],BigInteger arr[],Long x,int a,int b_index) {
		int low = 0;
	    int high = arr2.length-1;
	    int left=b_index;
	    int mid =0;
	    int right =arr2.length - b_index;
	    int left_moves = 0;
		int right_moves =0;
	    while(low <= high){
	    	mid = (low + high) / 2;
			        if(arr2[mid] .compareTo(BigInteger.valueOf(x))==0){
			            break;
			        }
			        else if(arr2[mid].compareTo(BigInteger.valueOf(x)) ==1  && a > mid){
			        	
			        	left_moves++;
			        	low = mid+1;
			        }
			        else if(arr2[mid] .compareTo(BigInteger.valueOf(x)) ==-1 && a > mid){
			            low = mid+1;
			            left--;
			        }
			        else if(arr2[mid].compareTo(BigInteger.valueOf(x)) ==1  && a < mid){
			            high = mid-1;
			            right--;
			            
			        }
			        else if(arr2[mid].compareTo(BigInteger.valueOf(x)) ==-1 && a < mid){
			        	right_moves++;
			        	high = mid-1;
			        }
	    }
	    check(left_moves,left,right_moves,right);
	}

}
