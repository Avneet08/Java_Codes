
public class hello {

	public static void main(String[] args) {
		int A[] = {6,15,10,3,4,0,6,15};
        int n =A.length;
         
        System.out.println(
                  largestGCD1Subset(A, n) );
	}
	 static int gcd(int a, int b)
	    {
	        if (a == 0)
	            return b;
	        return gcd(b % a, a);
	    }
	 
	    static int largestGCD1Subset(int A[],
	                                   int n)
	    {
	         
	        // finding gcd of whole array
	        int currentGCD = A[0];
	         
	        for (int i=1; i<n; i++)
	        {
	            currentGCD = 
	                    gcd(currentGCD, A[i]);
	     
	            if (currentGCD == 1)
	                return (A.length-n);
	        }
	     
	        return -1;
	    }

}
