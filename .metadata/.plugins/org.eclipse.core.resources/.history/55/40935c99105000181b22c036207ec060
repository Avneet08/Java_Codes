import java.util.Arrays;
import java.util.Scanner;

public class rw {

	public static void main(String args[]) {
		try {
			Scanner sc = new Scanner(System.in);
			int T = sc.nextInt();
			for (int j = 0; j < T; j++) {
				long limit = sc.nextInt();
				int arr[] = new int[(int) limit];
				for (int i = 0; i < limit; i++) {
					arr[i] = sc.nextInt();
				}
				int C[]=generateseq(arr,arr.length);
				//System.out.println(Arrays.toString(C));
				long Xor=0;
				for(int i=0;i<C.length;i++) {
					Xor=xoring(Xor,C[i]);
				}
				System.out.println(Xor);
			}
		} catch (Exception e) {
			return;
		}
}
	public static long xoring(long xor,int y){
		return (xor^y);
		}
	public static int[] generateseq(int[] arr,int n) {
		int[] B=new int[n*n];
		for(int i=0;i<n;i++) {
			for(int j=0;j<n;j++) {
				B[n*i+j]=arr[i]+arr[j];
				//System.out.println("b["+(n*i+j+1)+"] is "+B[n*i+j]);
			}
		}
		return B;
		
		
	}
}