import java.math.BigInteger;

public class Rough {
	public static int fib(int n){
	    double x = Math.sqrt(5)/5;
	    
	    double termOne = x * Math.pow((1+Math.sqrt(5))/2, n);
	    double termTwo = -x * Math.pow((1-Math.sqrt(5))/2, n);
	 
	    return (int)(termOne + termTwo);
	}
	public static void main(String[] args) {
		System.out.println(fib(100000000000));
		
	}

}
