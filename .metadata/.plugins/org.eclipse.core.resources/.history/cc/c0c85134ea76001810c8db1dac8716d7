import java.util.Scanner;

public class J1_p1 {
	static int find( int S[],int x,  int t )
	{
		if (t == 0)
			return 1;
		if (t < 0)
			return 0;
		if ( S.length <=0 && t >= 1)
			return 0;
		return find( S, x - 1, t ) +
			find( S,  x, t-S[ x-1] );
	}
	
	
	public static void main(String[] args) {
	
		
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		for (int i = 0; i < n; i++) {
			int t = sc.nextInt();
			int arr[] = {7, 3};
			
			if( find(arr,arr.length,  t)==0) {
				System.out.println("NO");
			}
			else {
				System.out.println("YES");
			}
			
			
	}
	}

}
