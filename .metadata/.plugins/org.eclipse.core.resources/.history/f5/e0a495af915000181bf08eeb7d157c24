import java.util.Arrays;
import java.util.Scanner;

public class r3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		for(int i =0;i<t;i++){
			int n = sc.nextInt();
			int k = sc.nextInt();
			int z = 0;
			int x =0;
			if(n%2 == 0){
				z = n/2;
				x = n/2;
			}
			else{
				z = n/2 + 1;
				x = n/2;
			}
			int motu[] = new int[z];
			int tomu[] = new int[x];
			int both[] = new int[n];
			int im = 0;
			int it =0;
			for(int j =0;j<n;j++){
				int p = 0;
				if(j%2 == 0){
					p = sc.nextInt();
					motu[im] = p;
					im++;
				}
				else{
					p = sc.nextInt();
					tomu[it] = p;
					it++;
				}
				both[j] = p;
			}
			Arrays.sort(motu);
			Arrays.sort(tomu);
			
			
			if(k>Math.min(z,x)){
				k = Math.min(z,x);
			}
			int sumTomu = 0;
			int sumMotu =0;
			for(int p =0;p<Math.max(motu.length,tomu.length);p++){
				if(p<motu.length && p<tomu.length) {
				    sumTomu = sumTomu + tomu[p];
				    sumMotu = sumMotu + motu[p];
				  } else if(p<tomu.length) {
					sumTomu = sumTomu + tomu[p];
				  } else {
					sumMotu = sumMotu + motu[p];
				  }
			}
			if(sumTomu > sumMotu){
				System.out.println("YES");
			}
			else{
				int temp =0;
				int temp2 =0;
				
				for(int u =0;u<k;u++){
						temp = tomu[u];
						temp2 = motu[motu.length-1-u];
						tomu[u] = temp2;
						motu[motu.length-1-u] = temp;
				}
				sumTomu = 0;
				sumMotu =0;
				for(int p =0;p<Math.max(motu.length,tomu.length);p++){
					if(p<motu.length && p<tomu.length) {
					    sumTomu = sumTomu + tomu[p];
					    sumMotu = sumMotu + motu[p];
					  } else if(p<tomu.length) {
						sumTomu = sumTomu + tomu[p];
					  } else {
						sumMotu = sumMotu + motu[p];
					  }
				}
				if(sumTomu > sumMotu){
					System.out.println("YES");
				}
				else{
					System.out.println("NO");
				}
			}
			
		}
			
			
		}

	
}
