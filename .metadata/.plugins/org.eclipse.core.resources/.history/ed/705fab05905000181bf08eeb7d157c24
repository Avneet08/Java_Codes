import java.util.Arrays;
import java.util.Scanner;

public class r3 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		for(int i =0;i<t;i++){
			int n = sc.nextInt();
			int k = sc.nextInt();
			int z = 0;
			int x =0;
			if(n%2 == 0){
				z = n/2;
				x = n/2;
			}
			else{
				z = n/2 + 1;
				x = n/2;
			}
			int motu[] = new int[z];
			int tomu[] = new int[x];
			int im = 0;
			int it =0;
			for(int j =0;j<n;j++){
				if(j%2 == 0){
					motu[im] = sc.nextInt();
					im++;
				}
				else{
					tomu[it] = sc.nextInt();
					it++;
				}
			}
			
			Arrays.sort(motu);
			Arrays.sort(tomu);
			
			int temp =0;
			int temp2 =0;
			int tempk = 0;
			if(k>min(tomu.length,motu.length)){
				tempk = k;
				k = min(tomu.length,motu.length);
			}
			for(int u =0;u<k;u++){
				temp = tomu[u];
				temp2 = motu[motu.length-1-u];
				tomu[u] = temp2;
				motu[motu.length-1-u] = temp;
			}
			if((tempk - k)%2 != 0){
				Arrays.sort(motu);
				Arrays.sort(tomu);
				
				temp = tomu[0];
				temp2 = motu[motu.length-1];
				tomu[0] = temp2;
				motu[motu.length-1] = temp;
			}
			int sumTomu = 0;
			int sumMotu =0;
			for(int p =0;p<max(motu.length,tomu.length);p++){
				if(p<motu.length && p<tomu.length) {
				    sumTomu = sumTomu + tomu[p];
				    sumMotu = sumMotu + motu[p];
				  } else if(p<tomu.length) {
					sumTomu = sumTomu + tomu[p];
				  } else {
					sumMotu = sumMotu + motu[p];
				  }
			}
//			for(int r =0;r<motu.length;r++){
//				System.out.println(motu[r]);
//			}
//			System.out.println("\n");
//			for(int r =0;r<tomu.length;r++){
//				System.out.println(tomu[r]);
//			}
			if(sumTomu > sumMotu){
				System.out.println("YES");
//				System.out.println(sumTomu);
//				System.out.println(sumMotu);
			}
			else{
				System.out.println("NO");
//				System.out.println(sumTomu);
//				System.out.println(sumMotu);
			}
		}
	}

	private static int min(int length, int length2) {
		if(length < length2){
			return length;
		}
		return length2;
	}

	private static int max(int length, int length2) {
		if(length > length2){
			return length;
		}
		return length2;
	}
}
